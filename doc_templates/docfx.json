{
  "metadata": [{
    "src": [{
      "src": "../Linq2Shadow",
      "files": ["Linq2Shadow.csproj"]
    }],
    "dest": "api",
    "disableGitFeatures": false,
    "disableDefaultFilter": false,
    "properties": {
      "TargetFramework": "netstandard2.0"
    }
  }],
  "build": {
    "content": [{
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["tour/**.md", "tour/**/toc.yml", "toc.yml", "*.md"]
      },
      {
        "files": ["get-started/**.md", "get-started/**/toc.yml", "toc.yml", "*.md"]
      },
      {
        "files": ["compatibility/**.md", "compatibility/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [{
      "files": ["images/**"]
    }],
    "overwrite": [{
      "files": ["apidoc/**.md"],
      "exclude": ["obj/**", "_site/**"]
    }],
    "dest": "../docs",
    "globalMetadata": {
      "_appTitle": "Linq2Shadow Documentation",
      "_appFooter": "Copyright Â© 2019 Dzianis Dziarkach",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.png",
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/DDzia/Linq2Shadow",
        "branch": "master"
      }
    },
    "fileMetadataFiles": [],
    "template": ["default", "templates/material"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}